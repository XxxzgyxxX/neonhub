-- Servi√ßos necess√°rios
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local VirtualInputManager = game:GetService("VirtualInputManager")
local TeleportService = game:GetService("TeleportService")

-- Vari√°veis principais
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

-- Configura√ß√µes do Neon Hub
local NeonConfig = {
    Version = "3.2.1",
    Creator = "XxxzgyxxX",
    Name = "üí´ NEON HUB",
    PrimaryColor = Color3.fromRGB(0, 255, 255), -- Ciano neon
    SecondaryColor = Color3.fromRGB(138, 43, 226), -- Roxo neon
    BackgroundColor = Color3.fromRGB(15, 15, 25),
    DarkColor = Color3.fromRGB(10, 10, 15),
    TextColor = Color3.fromRGB(255, 255, 255),
    AccentColor = Color3.fromRGB(255, 20, 147), -- Rosa neon
    SuccessColor = Color3.fromRGB(0, 255, 127),
    ErrorColor = Color3.fromRGB(255, 69, 0)
}

-- Vari√°veis de estado
local NeonState = {
    AutoFarm = false,
    AutoStats = false,
    AutoRaid = false,
    KillAura = false,
    NoClip = false,
    InfiniteJump = false,
    FastAttack = false,
    AutoQuest = false,
    FruitSniper = false,
    SeaEvents = false,
    SelectedQuest = nil,
    SelectedIsland = nil,
    WalkSpeed = 16,
    JumpPower = 50
}

-- Fun√ß√µes utilit√°rias
local function createTween(object, properties, duration, style)
    duration = duration or 0.3
    style = style or Enum.EasingStyle.Quart
    local info = TweenInfo.new(duration, style, Enum.EasingDirection.Out)
    return TweenService:Create(object, info, properties)
end

local function addCorner(object, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius or 8)
    corner.Parent = object
    return corner
end

local function addStroke(object, color, thickness)
    local stroke = Instance.new("UIStroke")
    stroke.Color = color or NeonConfig.PrimaryColor
    stroke.Thickness = thickness or 2
    stroke.Parent = object
    return stroke
end

local function addGradient(object, colors)
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new(colors)
    gradient.Parent = object
    return gradient
end

-- Sistema de notifica√ß√µes
local function sendNotification(title, message, duration, color)
    duration = duration or 3
    color = color or NeonConfig.SuccessColor
    
    local notification = Instance.new("Frame")
    notification.Name = "NeonNotification"
    notification.Size = UDim2.new(0, 350, 0, 80)
    notification.Position = UDim2.new(1, 370, 0, 20)
    notification.BackgroundColor3 = NeonConfig.DarkColor
    notification.BorderSizePixel = 0
    notification.Parent = playerGui
    
    addCorner(notification, 12)
    addStroke(notification, color, 2)
    addGradient(notification, {color, NeonConfig.DarkColor})
    
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -20, 0, 25)
    titleLabel.Position = UDim2.new(0, 10, 0, 5)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = title
    titleLabel.TextColor3 = color
    titleLabel.TextSize = 18
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = notification
    
    local messageLabel = Instance.new("TextLabel")
    messageLabel.Size = UDim2.new(1, -20, 0, 45)
    messageLabel.Position = UDim2.new(0, 10, 0, 25)
    messageLabel.BackgroundTransparency = 1
    messageLabel.Text = message
    messageLabel.TextColor3 = NeonConfig.TextColor
    messageLabel.TextSize = 14
    messageLabel.Font = Enum.Font.Gotham
    messageLabel.TextXAlignment = Enum.TextXAlignment.Left
    messageLabel.TextWrapped = true
    messageLabel.Parent = notification
    
    -- Anima√ß√£o
    createTween(notification, {Position = UDim2.new(1, -370, 0, 20)}):Play()
    
    wait(duration)
    createTween(notification, {Position = UDim2.new(1, 370, 0, 20)}):Play()
    wait(0.5)
    notification:Destroy()
end

-- Funcionalidades principais do Blox Fruits
local BloxFruits = {}

-- Auto Farm
function BloxFruits.AutoFarm()
    spawn(function()
        while NeonState.AutoFarm do
            wait(0.1)
            pcall(function()
                local enemies = Workspace.Enemies:GetChildren()
                for _, enemy in pairs(enemies) do
                    if enemy:FindFirstChild("HumanoidRootPart") and enemy:FindFirstChild("Humanoid") then
                        if enemy.Humanoid.Health > 0 then
                            -- Teleportar para o inimigo
                            rootPart.CFrame = enemy.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
                            
                            -- Atacar
                            VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 1)
                            wait(0.1)
                            VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 1)
                            
                            break
                        end
                    end
                end
            end)
        end
    end)
end

-- Auto Stats
function BloxFruits.AutoStats()
    spawn(function()
        while NeonState.AutoStats do
            wait(1)
            pcall(function()
                local stats = {"Melee", "Defense", "Sword", "Gun", "Fruit"}
                for _, stat in pairs(stats) do
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", stat, 1)
                end
            end)
        end
    end)
end

-- Kill Aura
function BloxFruits.KillAura()
    spawn(function()
        while NeonState.KillAura do
            wait(0.1)
            pcall(function()
                for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("HumanoidRootPart") and enemy:FindFirstChild("Humanoid") then
                        local distance = (rootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
                        if distance <= 50 and enemy.Humanoid.Health > 0 then
                            VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 1)
                            wait(0.1)
                            VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 1)
                        end
                    end
                end
            end)
        end
    end)
end

-- NoClip
function BloxFruits.NoClip()
    spawn(function()
        while NeonState.NoClip do
            wait()
            pcall(function()
                for _, part in pairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end
    end)
end

-- Fast Attack
function BloxFruits.FastAttack()
    spawn(function()
        while NeonState.FastAttack do
            wait()
            pcall(function()
                VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 1)
                VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 1)
            end)
        end
    end)
end

-- Teleport para ilhas
local Islands = {
    ["Starter Island"] = CFrame.new(945, 17, 1427),
    ["Marine Base"] = CFrame.new(-2573, 7, -3088),
    ["Jungle"] = CFrame.new(-1612, 37, 149),
    ["Pirate Village"] = CFrame.new(-1181, 5, 3803),
    ["Desert"] = CFrame.new(944, 21, 4481),
    ["Frozen Village"] = CFrame.new(1347, 104, -1319),
    ["Skylands"] = CFrame.new(-4607, 872, -1667),
    ["Prison"] = CFrame.new(4875, 6, 734),
    ["Colosseum"] = CFrame.new(-1427, 8, -2673),
    ["Magma Village"] = CFrame.new(-5247, 13, -2447),
    ["Underwater City"] = CFrame.new(61123, 18, 1569),
    ["Fountain City"] = CFrame.new(5127, 59, 4105),
    ["Shank Room"] = CFrame.new(-1442, 30, -15)
}

function BloxFruits.TeleportToIsland(islandName)
    if Islands[islandName] then
        rootPart.CFrame = Islands[islandName]
        sendNotification("Teleport", "Teleportado para " .. islandName, 2)
    end
end

-- Cria√ß√£o da interface
local function createNeonHub()
    -- Remover GUI existente
    if playerGui:FindFirstChild("NeonHubGUI") then
        playerGui.NeonHubGUI:Destroy()
    end
    
    -- ScreenGui principal
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "NeonHubGUI"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = playerGui
    
    -- Frame principal
    local mainFrame = Instance.new("Frame")
    mainFrame.Name = "MainFrame"
    mainFrame.Size = UDim2.new(0, 900, 0, 650)
    mainFrame.Position = UDim2.new(0.5, -450, 0.5, -325)
    mainFrame.BackgroundColor3 = NeonConfig.BackgroundColor
    mainFrame.BorderSizePixel = 0
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.Parent = screenGui
    
    addCorner(mainFrame, 15)
    addStroke(mainFrame, NeonConfig.PrimaryColor, 3)
    
    -- Efeito de brilho
    local glowEffect = Instance.new("ImageLabel")
    glowEffect.Name = "GlowEffect"
    glowEffect.Size = UDim2.new(1, 60, 1, 60)
    glowEffect.Position = UDim2.new(0, -30, 0, -30)
    glowEffect.BackgroundTransparency = 1
    glowEffect.Image = "rbxasset://textures/ui/Glow.png"
    glowEffect.ImageColor3 = NeonConfig.PrimaryColor
    glowEffect.ImageTransparency = 0.7
    glowEffect.Parent = mainFrame
    
    -- Header
    local header = Instance.new("Frame")
    header.Name = "Header"
    header.Size = UDim2.new(1, 0, 0, 80)
    header.Position = UDim2.new(0, 0, 0, 0)
    header.BackgroundColor3 = NeonConfig.DarkColor
    header.BorderSizePixel = 0
    header.Parent = mainFrame
    
    addCorner(header, 15)
    addGradient(header, {NeonConfig.PrimaryColor, NeonConfig.SecondaryColor})
    
    -- T√≠tulo
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Name = "Title"
    titleLabel.Size = UDim2.new(0, 400, 1, 0)
    titleLabel.Position = UDim2.new(0, 20, 0, 0)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = NeonConfig.Name
    titleLabel.TextColor3 = NeonConfig.TextColor
    titleLabel.TextSize = 32
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = header
    
    -- Informa√ß√µes
    local infoLabel = Instance.new("TextLabel")
    infoLabel.Name = "Info"
    infoLabel.Size = UDim2.new(0, 300, 0, 30)
    infoLabel.Position = UDim2.new(1, -320, 0, 15)
    infoLabel.BackgroundTransparency = 1
    infoLabel.Text = "v" .. NeonConfig.Version .. " | " .. NeonConfig.Creator
    infoLabel.TextColor3 = NeonConfig.TextColor
    infoLabel.TextSize = 16
    infoLabel.Font = Enum.Font.Gotham
    infoLabel.TextXAlignment = Enum.TextXAlignment.Right
    infoLabel.Parent = header
    
    -- Status
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Name = "Status"
    statusLabel.Size = UDim2.new(0, 200, 0, 25)
    statusLabel.Position = UDim2.new(1, -220, 0, 45)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "üü¢ ONLINE | Premium User"
    statusLabel.TextColor3 = NeonConfig.SuccessColor
    statusLabel.TextSize = 14
    statusLabel.Font = Enum.Font.GothamBold
    statusLabel.TextXAlignment = Enum.TextXAlignment.Right
    statusLabel.Parent = header
    
    -- Bot√£o fechar
    local closeButton = Instance.new("TextButton")
    closeButton.Name = "CloseButton"
    closeButton.Size = UDim2.new(0, 40, 0, 40)
    closeButton.Position = UDim2.new(1, -50, 0, 10)
    closeButton.BackgroundColor3 = NeonConfig.ErrorColor
    closeButton.Text = "‚úï"
    closeButton.TextColor3 = NeonConfig.TextColor
    closeButton.TextSize = 20
    closeButton.Font = Enum.Font.GothamBold
    closeButton.BorderSizePixel = 0
    closeButton.Parent = header
    
    addCorner(closeButton, 20)
    
    closeButton.MouseButton1Click:Connect(function()
        createTween(mainFrame, {Size = UDim2.new(0, 0, 0, 0)}):Play()
        wait(0.3)
        screenGui:Destroy()
    end)
    
    -- Container de abas
    local tabContainer = Instance.new("Frame")
    tabContainer.Name = "TabContainer"
    tabContainer.Size = UDim2.new(0, 200, 1, -80)
    tabContainer.Position = UDim2.new(0, 0, 0, 80)
    tabContainer.BackgroundColor3 = NeonConfig.DarkColor
    tabContainer.BorderSizePixel = 0
    tabContainer.Parent = mainFrame
    
    -- √Årea de conte√∫do
    local contentArea = Instance.new("Frame")
    contentArea.Name = "ContentArea"
    contentArea.Size = UDim2.new(1, -200, 1, -80)
    contentArea.Position = UDim2.new(0, 200, 0, 80)
    contentArea.BackgroundColor3 = NeonConfig.BackgroundColor
    contentArea.BorderSizePixel = 0
    contentArea.Parent = mainFrame
    
    -- Fun√ß√£o para criar bot√µes de aba
    local function createTabButton(name, icon, position, callback)
        local button = Instance.new("TextButton")
        button.Name = name .. "Tab"
        button.Size = UDim2.new(1, -10, 0, 50)
        button.Position = UDim2.new(0, 5, 0, position)
        button.BackgroundColor3 = Color3.fromRGB(255, 255, 255, 0.1)
        button.Text = icon .. " " .. name
        button.TextColor3 = NeonConfig.TextColor
        button.TextSize = 16
        button.Font = Enum.Font.Gotham
        button.TextXAlignment = Enum.TextXAlignment.Left
        button.BorderSizePixel = 0
        button.Parent = tabContainer
        
        addCorner(button, 8)
        
        local padding = Instance.new("UIPadding")
        padding.PaddingLeft = UDim.new(0, 15)
        padding.Parent = button
        
        button.MouseEnter:Connect(function()
            createTween(button, {BackgroundColor3 = Color3.fromRGB(255, 255, 255, 0.2)}):Play()
        end)
        
        button.MouseLeave:Connect(function()
            createTween(button, {BackgroundColor3 = Color3.fromRGB(255, 255, 255, 0.1)}):Play()
        end)
        
        button.MouseButton1Click:Connect(callback)
        
        return button
    end
    
    -- Fun√ß√£o para criar toggle
    local function createToggle(parent, name, position, callback)
        local toggleFrame = Instance.new("Frame")
        toggleFrame.Name = name .. "Toggle"
        toggleFrame.Size = UDim2.new(1, -20, 0, 50)
        toggleFrame.Position = UDim2.new(0, 10, 0, position)
        toggleFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255, 0.05)
        toggleFrame.BorderSizePixel = 0
        toggleFrame.Parent = parent
        
        addCorner(toggleFrame, 8)
        
        local toggleLabel = Instance.new("TextLabel")
        toggleLabel.Size = UDim2.new(1, -80, 1, 0)
        toggleLabel.Position = UDim2.new(0, 15, 0, 0)
        toggleLabel.BackgroundTransparency = 1
        toggleLabel.Text = name
        toggleLabel.TextColor3 = NeonConfig.TextColor
        toggleLabel.TextSize = 16
        toggleLabel.Font = Enum.Font.Gotham
        toggleLabel.TextXAlignment = Enum.TextXAlignment.Left
        toggleLabel.Parent = toggleFrame
        
        local toggleButton = Instance.new("TextButton")
        toggleButton.Size = UDim2.new(0, 60, 0, 30)
        toggleButton.Position = UDim2.new(1, -70, 0.5, -15)
        toggleButton.BackgroundColor3 = NeonConfig.ErrorColor
        toggleButton.Text = "OFF"
        toggleButton.TextColor3 = NeonConfig.TextColor
        toggleButton.TextSize = 14
        toggleButton.Font = Enum.Font.GothamBold
        toggleButton.BorderSizePixel = 0
        toggleButton.Parent = toggleFrame
        
        addCorner(toggleButton, 15)
        
        local isToggled = false
        
        toggleButton.MouseButton1Click:Connect(function()
            isToggled = not isToggled
            if isToggled then
                toggleButton.BackgroundColor3 = NeonConfig.SuccessColor
                toggleButton.Text = "ON"
            else
                toggleButton.BackgroundColor3 = NeonConfig.ErrorColor
                toggleButton.Text = "OFF"
            end
            callback(isToggled)
        end)
        
        return toggleFrame
    end
    
    -- Fun√ß√£o para criar bot√£o simples
    local function createButton(parent, name, position, callback)
        local button = Instance.new("TextButton")
        button.Name = name .. "Button"
        button.Size = UDim2.new(1, -20, 0, 40)
        button.Position = UDim2.new(0, 10, 0, position)
        button.BackgroundColor3 = NeonConfig.PrimaryColor
        button.Text = name
        button.TextColor3 = NeonConfig.DarkColor
        button.TextSize = 16
        button.Font = Enum.Font.GothamBold
        button.BorderSizePixel = 0
        button.Parent = parent
        
        addCorner(button, 8)
        
        button.MouseEnter:Connect(function()
            createTween(button, {BackgroundColor3 = NeonConfig.SecondaryColor}):Play()
        end)
        
        button.MouseLeave:Connect(function()
            createTween(button, {BackgroundColor3 = NeonConfig.PrimaryColor}):Play()
        end)
        
        button.MouseButton1Click:Connect(callback)
        
        return button
    end
    
    -- Fun√ß√£o para limpar conte√∫do
    local function clearContent()
        for _, child in pairs(contentArea:GetChildren()) do
            if child.Name ~= "ContentScroll" then
                child:Destroy()
            end
        end
    end
    
    -- Scroll frame para conte√∫do
    local contentScroll = Instance.new("ScrollingFrame")
    contentScroll.Name = "ContentScroll"
    contentScroll.Size = UDim2.new(1, -20, 1, -20)
    contentScroll.Position = UDim2.new(0, 10, 0, 10)
    contentScroll.BackgroundTransparency = 1
    contentScroll.ScrollBarThickness = 8
    contentScroll.ScrollBarImageColor3 = NeonConfig.PrimaryColor
    contentScroll.Parent = contentArea
    
    -- Abas do hub
    local function showFarmingTab()
        clearContent()
        contentScroll.CanvasSize = UDim2.new(0, 0, 0, 400)
        
        createToggle(contentScroll, "Auto Farm", 10, function(state)
            NeonState.AutoFarm = state
            if state then
                BloxFruits.AutoFarm()
                sendNotification("Auto Farm", "Auto Farm ativado!", 2)
            else
                sendNotification("Auto Farm", "Auto Farm desativado!", 2)
            end
        end)
        
        createToggle(contentScroll, "Fast Attack", 70, function(state)
            NeonState.FastAttack = state
            if state then
                BloxFruits.FastAttack()
                sendNotification("Fast Attack", "Ataque r√°pido ativado!", 2)
            else
                sendNotification("Fast Attack", "Ataque r√°pido desativado!", 2)
            end
        end)
        
        createToggle(contentScroll, "Kill Aura", 130, function(state)
            NeonState.KillAura = state
            if state then
                BloxFruits.KillAura()
                sendNotification("Kill Aura", "Kill Aura ativado!", 2)
            else
                sendNotification("Kill Aura", "Kill Aura desativado!", 2)
            end
        end)
        
        createToggle(contentScroll, "Auto Quest", 190, function(state)
            NeonState.AutoQuest = state
            sendNotification("Auto Quest", state and "Ativado!" or "Desativado!", 2)
        end)
        
        createToggle(contentScroll, "Auto Stats", 250, function(state)
            NeonState.AutoStats = state
            if state then
                BloxFruits.AutoStats()
                sendNotification("Auto Stats", "Auto Stats ativado!", 2)
            else
                sendNotification("Auto Stats", "Auto Stats desativado!", 2)
            end
        end)
    end
    
    local function showPlayerTab()
        clearContent()
        contentScroll.CanvasSize = UDim2.new(0, 0, 0, 350)
        
        createToggle(contentScroll, "NoClip", 10, function(state)
            NeonState.NoClip = state
            if state then
                BloxFruits.NoClip()
                sendNotification("NoClip", "NoClip ativado!", 2)
            else
                sendNotification("NoClip", "NoClip desativado!", 2)
            end
        end)
        
        createToggle(contentScroll, "Infinite Jump", 70, function(state)
            NeonState.InfiniteJump = state
            sendNotification("Infinite Jump", state and "Ativado!" or "Desativado!", 2)
        end)
        
        -- Speed slider
        local speedFrame = Instance.new("Frame")
        speedFrame.Size = UDim2.new(1, -20, 0, 60)
        speedFrame.Position = UDim2.new(0, 10, 0, 130)
        speedFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255, 0.05)
        speedFrame.BorderSizePixel = 0
        speedFrame.Parent = contentScroll
        
        addCorner(speedFrame, 8)
        
        local speedLabel = Instance.new("TextLabel")
        speedLabel.Size = UDim2.new(1, 0, 0, 30)
        speedLabel.Position = UDim2.new(0, 0, 0, 0)
        speedLabel.BackgroundTransparency = 1
        speedLabel.Text = "Walk Speed: " .. NeonState.WalkSpeed
        speedLabel.TextColor3 = NeonConfig.TextColor
        speedLabel.TextSize = 16
        speedLabel.Font = Enum.Font.Gotham
        speedLabel.Parent = speedFrame
        
        local speedSlider = Instance.new("TextButton")
        speedSlider.Size = UDim2.new(1, -20, 0, 20)
        speedSlider.Position = UDim2.new(0, 10, 0, 35)
        speedSlider.BackgroundColor3 = NeonConfig.DarkColor
        speedSlider.Text = ""
        speedSlider.BorderSizePixel = 0
        speedSlider.Parent = speedFrame
        
        addCorner(speedSlider, 10)
        addStroke(speedSlider, NeonConfig.PrimaryColor, 2)
    end
    
    local function showTeleportTab()
        clearContent()
        contentScroll.CanvasSize = UDim2.new(0, 0, 0, 600)
        
        local y = 10
        for islandName, _ in pairs(Islands) do
            createButton(contentScroll, islandName, y, function()
                BloxFruits.TeleportToIsland(islandName)
            end)
            y = y + 50
        end
    end
    
    local function showMiscTab()
        clearContent()
        contentScroll.CanvasSize = UDim2.new(0, 0, 0, 300)
        
        createToggle(contentScroll, "Auto Raid", 10, function(state)
            NeonState.AutoRaid = state
            sendNotification("Auto Raid", state and "Ativado!" or "Desativado!", 2)
        end)
        
        createToggle(contentScroll, "Fruit Sniper", 70, function(state)
            NeonState.FruitSniper = state
            sendNotification("Fruit Sniper", state and "Ativado!" or "Desativado!", 2)
        end)
        
        createToggle(contentScroll, "Sea Events", 130, function(state)
            NeonState.SeaEvents = state
            sendNotification("Sea Events", state and "Ativado!" or "Desativado!", 2)
        end)
        
        createButton(contentScroll, "Rejoin Server", 190, function()
            TeleportService:Teleport(game.PlaceId, player)
        end)
        
        createButton(contentScroll, "Server Hop", 240, function()
